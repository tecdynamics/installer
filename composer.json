{
    "name": "tecdynamics/cms",
    "description": "Tec - Laravel web installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Michail Fragkiskos",
            "homepage": "https://github.com/tecdynamics/installer",
            "role": "Manager",
